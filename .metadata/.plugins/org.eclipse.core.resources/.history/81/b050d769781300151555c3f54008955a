package task1.tests;

import static org.junit.Assert.*;

import java.util.Random;

import org.junit.BeforeClass;
import org.junit.Test;

public class SearchTests {
	private static final int NUMBER_OF_ARRAYS = 10;
	private static final int ARRAY_BOUND = 100;
	@BeforeClass
	public static void setUpBeforeClass() throws Exception {
		
	}

	@Test
	public void test() {
		Random random = new Random();
		for (int i = 0; i < NUMBER_OF_ARRAYS; i++) {
			System.out.println(TestGenerator.isContain(TestGenerator.generateTestArray(random.nextInt(ARRAY_BOUND)), random.nextInt(ARRAY_BOUND)));
		}
	}

}
