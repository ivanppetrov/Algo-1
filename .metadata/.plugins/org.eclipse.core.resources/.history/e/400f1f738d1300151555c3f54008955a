package task1.solutions;

public class Roots {
	public static double squareRoot(int number) {
		double precision = 0.00001;
		double result = -1d;
		int low = 1;		
		int high = number;
		int mid = low + (high - low) / 2;
		
		while (result == number) {
			result = mid * mid;
			if (result <= number) {
				high = mid;
			} else {
				low = mid + 1;
			}
			
			mid = low + (high - low) / 2;
		}
		
		return result;
	}
}
