package task2.tests;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

import task2.solutions.BirthadyRanges;
import task2.solutions.Pair;

public class Tests {

	public static void main(String[] args) {
		int[] birthdays = new int[]{1, 3, 14, 5, 3, 1, 7, 7, 3, 14};
		Pair pair = new Pair(2, 7);
		List<Pair> ranges = new ArrayList<Pair>();
		ranges.add(pair)
		BirthadyRanges.countBirthdays(Arrays.asList(birthdays), ranges);

	}

}
